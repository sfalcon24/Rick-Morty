{
  "name": "rnapp",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "starter": "exec ./scripts/starter.sh",
    "postinstall": "husky install && patch-package",
    "bump": "sh ./scripts/bump-version.sh",
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "android:stg": "ENVFILE=.env.stg yarn android",
    "ios:stg": "ENVFILE=.env.stg yarn ios",
    "android:prod": "ENVFILE=.env.prod yarn android",
    "ios:prod": "ENVFILE=.env.prod yarn ios",
    "start": "react-native start",
    "test": "TZ=UTC node ./.storybook/jestSetup.js && TZ=UTC jest --runInBand --passWithNoTests",
    "lint": "tsc --noEmit && eslint --cache 'src/**/*.ts?(x)'",
    "lint:fix": "yarn lint --fix",
    "lint:staged": "yarn lint-staged",
    "codegen": "rm -rf src/apollo/generated && graphql-codegen",
    "open:xcode": "open ios/rnapp.xcworkspace/",
    "sb:on": "exec ./scripts/storybook-enabler.sh on",
    "sb:off": "exec ./scripts/storybook-enabler.sh off",
    "sb:getstories": "sb-rn-get-stories",
    "sb:watcher": "sb-rn-watcher"
  },
  "dependencies": {
    "@apollo/client": "3.7.14",
    "@hookform/resolvers": "3.1.0",
    "@react-navigation/bottom-tabs": "6.5.7",
    "@react-navigation/native": "6.1.6",
    "@react-navigation/native-stack": "6.9.12",
    "apollo3-cache-persist": "0.14.1",
    "expo": "48.0.0",
    "i18next": "22.5.0",
    "intl-pluralrules": "2.0.0",
    "lodash": "4.17.21",
    "react": "18.2.0",
    "react-hook-form": "7.43.9",
    "react-i18next": "12.3.1",
    "react-native": "0.71.3",
    "react-native-config": "1.5.1",
    "react-native-gesture-handler": "2.9.0",
    "react-native-mmkv": "2.8.0",
    "react-native-reanimated": "2.14.4",
    "react-native-safe-area-context": "4.5.0",
    "react-native-screens": "3.20.0",
    "react-native-svg": "13.4.0",
    "styled-components": "5.3.10",
    "yup": "1.1.1"
  },
  "devDependencies": {
    "@babel/core": "7.20.2",
    "@babel/preset-env": "7.20.2",
    "@babel/runtime": "7.20.1",
    "@commitlint/cli": "17.6.3",
    "@commitlint/config-conventional": "17.6.3",
    "@graphql-codegen/add": "4.0.1",
    "@graphql-codegen/cli": "3.3.1",
    "@graphql-codegen/fragment-matcher": "4.0.1",
    "@graphql-codegen/typed-document-node": "4.0.1",
    "@graphql-codegen/typescript-apollo-client-helpers": "2.2.6",
    "@graphql-codegen/typescript-document-nodes": "3.0.4",
    "@graphql-codegen/typescript-operations": "3.0.4",
    "@graphql-codegen/typescript-react-apollo": "3.3.7",
    "@react-native-async-storage/async-storage": "1.17.11",
    "@react-native-community/datetimepicker": "6.7.4",
    "@react-native-community/eslint-config": "3.2.0",
    "@react-native-community/slider": "4.4.2",
    "@storybook/addon-actions": "6.5.3",
    "@storybook/addon-ondevice-actions": "6.5.3",
    "@storybook/addon-ondevice-backgrounds": "6.5.3",
    "@storybook/addon-ondevice-controls": "6.5.3",
    "@storybook/addon-ondevice-notes": "6.5.3",
    "@storybook/react-native": "6.5.3",
    "@testing-library/react-hooks": "8.0.1",
    "@tsconfig/react-native": "3.0.0",
    "@types/jest": "29.2.4",
    "@types/lodash": "4.14.194",
    "@types/react": "18.0.24",
    "@types/react-native": "0.72.0",
    "@types/react-test-renderer": "18.0.0",
    "@types/styled-components-react-native": "5.2.0",
    "@typescript-eslint/eslint-plugin": "5.43.0",
    "@typescript-eslint/parser": "5.43.0",
    "babel-jest": "29.3.1",
    "babel-plugin-module-resolver": "4.1.0",
    "eslint": "8.27.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jest": "27.1.6",
    "husky": "8.0.3",
    "jest": "29.3.1",
    "lint-staged": "13.0.3",
    "metro-react-native-babel-preset": "0.73.7",
    "patch-package": "7.0.0",
    "react-dom": "18.2.0",
    "react-native-dotenv": "3.4.8",
    "react-native-flipper": "0.191.0",
    "react-native-flipper-apollo-devtools": "0.0.2",
    "react-native-mmkv-flipper-plugin": "1.0.0",
    "react-test-renderer": "18.2.0",
    "typescript": "4.8.4"
  },
  "resolutions": {
    "@types/react-native": "0.72.0"
  }
}
